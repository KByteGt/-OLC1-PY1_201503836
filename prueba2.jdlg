CREAR TABLA Departamento(
	Id_departamento entero,
	nombre cadena
);

INSERTAR EN Departamento VALORES(0,"Alta Verapaz");
INSERTAR EN Departamento VALORES(1,"Baja Verapaz");

SELECCIONAR Zona.Nombre como Zona, Municipio.Nombre como Municipio,
Departamento.Nombre como Departamento
DE Zona, Municipio, Departamento
DONDE Zona.id_municipio=Municipio.id_municipio
Y Municipio.id_departamento=Departamento.id_departamento

SELECCIONAR * DE zona
DONDE zona.id_municipio = 3
y zona.id_municipio != 4
